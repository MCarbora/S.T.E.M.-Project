trace0 = go.Scatter(
    x = Year,
    y = Land,
    name = 'Land',
    line = dict(
        color = ('rgb(205, 12, 24)'),
        width = 4))

trace1 = go.Scatter(
    x = Year,
    y = Land_and_Ocean,
    name = 'Land and Ocean',
    line = dict(
        color = ('rgb(22, 96, 167)'),
        width = 4,))

trace2 = go.Scatter(
    x = Year,
    y = N_Hem,
    name = 'N Hem',
    line = dict(
        color = ('rgb(205, 12, 24)'),
        width = 4,
        dash = 'dash'))

trace3 = go.Scatter(
    x = Year,
    y = S_Hem,
    name = 'S Hem',
    line = dict(
        color = ('rgb(22, 96, 167)'),
        width = 4,
        dash = 'dash'))

trace4 = go.Scatter(
    x = Year,
    y = Band_1,
    name = 'Band 1',
    line = dict(
        color = ('rgb(205, 12, 24)'),
        width = 4,
        dash = 'dot'))

trace5 = go.Scatter(
    x = Year,
    y = Band_2,
    name = 'Band 2',
    line = dict(
        color = ('rgb(22, 96, 167)'),
        width = 4,
        dash = 'dot'))

trace6 = go.Scatter(
     x = Year,
     y = Band_3,
     name = 'Band 3',
     line = dict(
        color = ('rgb(205, 12, 24)'),
        width = 4,
        dash = 'dashdot'))

data = [trace0, trace1, trace2, trace3, trace4, trace5, trace6]


layout = dict(title = 'Temperature Anomalies Worldwide',
              xaxis = dict(title = 'Year'),
              yaxis = dict(title = 'Temperature Anomalies'))


fig = dict(data = data, layout = layout)
py.iplot(fig, filename = 'styled-line')
